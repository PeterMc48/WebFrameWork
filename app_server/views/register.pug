extends layout

block content
  .container
    h2 Register
    form(action='')
      .form-group
        label First Name: 
        input(type='text' name='firstName')
      .form-group
        label Last Name: 
        input(type='text' name='lastName')
      .form-group
        label E-Mail: 
        input(type='text' name='eMail')
      .form-group
        label Password: 
        input(type='text' name='password') 

      button.btn.btn-primary(type='submit') Submit  